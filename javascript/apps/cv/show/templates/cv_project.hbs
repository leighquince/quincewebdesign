<div class="cv-section-details row">
  <div class="project-section-name large-6 columns">
    {{#if website}}
    <h4> <a href="{{website}}" target="_blank">
      {{name}}
    </a>
    </h4>
    {{else}}

    <h4>{{name}}</h4>
    {{/if}}
  </div>

  <div class="project-section-dates large-6 columns">
    <div class="project-section-start-date date-stamp">
      <p>{{HB_momentFormatDate startDate}}</p>
    </div>
  </div>
</div>
<div class="project-section-highlights row">
  <ul class="project-section-highlight-list">
    {{#each highlights}}
    <li class="project-section-highlight-list-item">{{this}}</li>
    {{/each}}
  </ul>
</div>
